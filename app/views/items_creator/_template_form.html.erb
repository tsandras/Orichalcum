New Template :
<%= form_for @template, url: items_creator_save_template_path, html: { multipart: true } do |form| %>
  Name :
  <%= form.text_field :name %><br>
  Rarity :
  <%= form.text_field :rarity %><br>
  <%= form.file_field :image %><br>
  Description :
  <%= form.text_field :description %><br>
  <%= form.submit %>
<% end %>
